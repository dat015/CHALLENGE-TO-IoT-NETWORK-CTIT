@{
    ViewData["Title"] = "Trang chủ";
}

<!-- Hero Section -->
<div class="hero-section position-relative mb-5">
    <div class="container">
        <div class="row align-items-center min-vh-75">
            <div class="col-lg-6">
                <h1 class="display-4 fw-bold text-primary mb-4">Nông sản Xanh - Truy xuất nguồn gốc minh bạch</h1>
                <p class="lead mb-4">Hệ thống quản lý nông sản thông minh với công nghệ blockchain, giúp nông dân quản
                    lý hiệu quả và người tiêu dùng truy xuất nguồn gốc dễ dàng.</p>
                <div class="d-flex gap-3">
                    <a href="#trace" class="btn btn-success btn-lg">
                        <i class="fas fa-qrcode me-2"></i>Truy xuất nguồn gốc
                    </a>
                    <a href="#features" class="btn btn-outline-success btn-lg">
                        <i class="fas fa-info-circle me-2"></i>Tìm hiểu thêm
                    </a>
                </div>
            </div>
            <div class="col-lg-6">
                <img style ="height: 389px; width: 600px;" src="data:image/jpeg;base64,"
                    alt="Nông sản Xanh" class="img-fluid rounded-3 shadow-lg">
            </div>
        </div>
    </div>
</div>

<!-- Features Section -->
<div id="features" class="py-5">
    <div class="container">
        <div class="text-center mb-5">
            <h2 class="display-5 fw-bold text-primary">Tính năng nổi bật</h2>
            <p class="lead text-muted">Giải pháp toàn diện cho nông nghiệp thông minh</p>
        </div>
        <div class="row g-4">
            <div class="col-md-4">
                <div class="card h-100 border-0 shadow-sm">
                    <div class="card-body text-center p-4">
                        <div class="feature-icon mb-4">
                            <i class="fas fa-seedling fa-3x text-success"></i>
                        </div>
                        <h3 class="h5 fw-bold mb-3">Quản lý cây trồng</h3>
                        <p class="text-muted mb-0">Theo dõi toàn bộ quá trình sinh trưởng và phát triển của cây trồng.
                        </p>
                    </div>
                </div>
            </div>
            <div class="col-md-4">
                <div class="card h-100 border-0 shadow-sm">
                    <div class="card-body text-center p-4">
                        <div class="feature-icon mb-4">
                            <i class="fas fa-microchip fa-3x text-success"></i>
                        </div>
                        <h3 class="h5 fw-bold mb-3">Cảm biến thông minh</h3>
                        <p class="text-muted mb-0">Thu thập dữ liệu môi trường tự động, giúp tối ưu điều kiện canh tác.
                        </p>
                    </div>
                </div>
            </div>
            <div class="col-md-4">
                <div class="card h-100 border-0 shadow-sm">
                    <div class="card-body text-center p-4">
                        <div class="feature-icon mb-4">
                            <i class="fas fa-qrcode fa-3x text-success"></i>
                        </div>
                        <h3 class="h5 fw-bold mb-3">Truy xuất nguồn gốc</h3>
                        <p class="text-muted mb-0">Quét mã QR để xem thông tin chi tiết về nguồn gốc nông sản.</p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<!-- Trace Section -->
<div id="trace" class="py-5 bg-light">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-lg-8 text-center mb-5">
                <h2 class="display-5 fw-bold text-primary">Truy xuất nguồn gốc</h2>
                <p class="lead text-muted">Quét mã QR hoặc nhập mã sản phẩm để xem thông tin chi tiết</p>
            </div>
        </div>
        <div class="row justify-content-center">
            <div class="col-lg-6">
                <div class="card border-0 shadow-sm">
                    <div class="card-body p-4">
                        <form asp-controller="Trace" asp-action="GetTrace" method="get">
                            <div class="input-group mb-3">
                                <input type="text" name="code" class="form-control form-control-lg"
                                    placeholder="Nhập mã sản phẩm">
                                <button class="btn btn-success" type="submit">
                                    <i class="fas fa-search me-2"></i>Tìm kiếm
                                </button>
                            </div>
                            <div class="text-center">
                                <button type="button" class="btn btn-outline-success" data-bs-toggle="modal"
                                    data-bs-target="#scanQRModal">
                                    <i class="fas fa-qrcode me-2"></i>Quét mã QR
                                </button>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<!-- Farmer Section -->
<div class="py-5">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-lg-8 text-center mb-5">
                <h2 class="display-5 fw-bold text-primary">Dành cho nông dân</h2>
                <p class="lead text-muted">Đăng ký tài khoản để quản lý nông trại thông minh</p>
            </div>
        </div>
        <div class="row justify-content-center">
            <div class="col-lg-6">
                <div class="card border-0 shadow-sm">
                    <div class="card-body p-4 text-center">
                        <div class="mb-4">
                            <i class="fas fa-user-farmer fa-3x text-success"></i>
                        </div>
                        <h3 class="h4 fw-bold mb-3">Quản lý nông trại thông minh</h3>
                        <p class="text-muted mb-4">Truy cập các tính năng quản lý nông trại, theo dõi cây trồng và tạo
                            mã QR cho sản phẩm.</p>
                        <a asp-controller="Account" asp-action="Register" class="btn btn-success btn-lg">
                            <i class="fas fa-user-plus me-2"></i>Đăng ký tài khoản
                        </a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<!-- QR Scanner Modal -->
<div class="modal fade" id="scanQRModal" tabindex="-1">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Quét mã QR</h5>
                <button type="button" class="btn-close" data-bs-dismiss="modal"></button>
            </div>
            <div class="modal-body">
                <div id="reader"></div>
            </div>
        </div>
    </div>
</div>

@section Scripts {
    <script src="https://unpkg.com/html5-qrcode"></script>
    <script>
        function onScanSuccess(decodedText, decodedResult) {
            window.location.href = `${decodedText}`;
        }

        function onScanFailure(error) {
            console.warn(`QR scan failed: ${error}`);
        }

        let html5QrcodeScanner = new Html5QrcodeScanner(
            "reader",
            {
                fps: 10,
                qrbox: { width: 250, height: 250 },
            },
            false
        );

        html5QrcodeScanner.render(onScanSuccess, onScanFailure);
    </script>
}
